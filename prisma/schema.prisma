generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
}

model Role {
  id_role String  @id @unique
  name    String?
  

}

model ProfileMahasiswa {
  id_user        String  @id @unique
  name_mahasiswa String?
  no_telphone      String?
  gender         String?
  
}
