generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
}


model User {
  username_email   String? @map("username / email")
  password         String?
  id_user          String  @id
  id_role          String?
  email            String?
  

}

model Counselorpsychologist {
  id_user           String  @id @unique
  name_psychologist String? @map("name_psikolog / konselor")
  jwork_schedule    String?
  status            String?
  work_experiance   String?
  specialization    String?

}


model Role {
  id_role String  @id @unique
  name    String?
  

}

model ProfileMahasiswa {
  id_user        String  @id @unique
  name_mahasiswa String?
  no_telphone      String?
  gender         String?
  
}

model Article {
  id_article String  @id @unique
  title      String?
  category   String?
  content    String?
  
}
